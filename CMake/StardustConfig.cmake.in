@PACKAGE_INIT@

set(STARDUST_DEPENDS_INCLUDE_DIRS @PACKAGE_STARDUST_DEPENDS_INCLUDES@ )
set(FMOD_LIBRARY @PACKAGE_FMOD_LIB@)

foreach( INCLUDE_DIR ${STARDUST_DEPENDS_INCLUDE_DIRS} )
    set_and_check(TEMP_DIR ${INCLUDE_DIR})
    list( APPEND STARDUST_INCLUDE_DIRECTORIES ${TEMP_DIR} )
endforeach()

if( NOT TARGET stardust )
	include(${CMAKE_CURRENT_LIST_DIR}/stardustTargets.cmake)
endif()

set(STARDUST_FOUND TRUE)
set(STARDUST_INCLUDE_DIRS ${STARDUST_INCLUDE_DIRECTORIES})
set(STARDUST_LIBRARIES stardust ${STARDUST_DEPENDS_LIBS})

